{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": [ "error", "always" ],
        "quote-props": [ "off" ],
        "no-undef-init": [ "off" ]
    }
}
